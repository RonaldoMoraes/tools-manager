{
  "name": "tools-manager",
  "version": "1.0.0",
  "description": "An API for my Dev Tools Management",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev server/server.ts",
    "start": "npm run build && node dist/server/server.js",
    "build": "tsc",
    "test": "jest --no-cache --detectOpenHandles",
    "test:watch": "jest --coverage --watchAll --no-cache --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RonaldoMoraes/tools-manager-test.git"
  },
  "keywords": [
    "tools"
  ],
  "author": "Ronaldo Moraes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RonaldoMoraes/tools-manager-test/issues"
  },
  "homepage": "https://github.com/RonaldoMoraes/tools-manager-test#readme",
  "dependencies": {
    "@typegoose/typegoose": "^7.3.2",
    "@types/jest": "^26.0.10",
    "@types/supertest": "^2.0.10",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "mongoose": "^5.10.0",
    "objection": "^2.2.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.19",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "commitizen": "^4.1.5",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "sqlite3": "^5.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
